{
	"type": "config",
	"id": "conf.service.remotedesktop",
	"title": "Remote Desktop",
	"queryinfo": {
		"xpath": "//services/remotedesktop",
		"iterable": false
	},
	"properties": {
		"rdpport": {
			"type": "integer",
			"minimum": 1,
			"maximum": 65535,
			"default": 3389
		},
		"cryptlevel": {
			"type": "string",
			"enum": [
				"low",
				"medium",
				"high"
			]
		}
	}
}
